{
  "$schema": "https://raw.githubusercontent.com/grafana/grafana/master/docs/sources/developers/plugins/plugin.schema.json",
  "type": "datasource",
  "name": "Netic Humio",
  "id": "netic-humio-datasource",
  "category": "logging",

  "logs": true,
  "metrics": true,

  "info": {
    "description": "Netic Grafana Data Source for Humio logs",
    "author": {
      "name": "Netic",
      "url": "https://netic.dk"
    },
    "keywords": ["Simple"],
    "logos": {
      "small": "img/logo.svg",
      "large": "img/logo.svg"
    },
    "links": [
      { "name": "Website", "url": "https://github.com/grafana/grafana-starter-datasource" },
      { "name": "License", "url": "https://github.com/grafana/grafana-starter-datasource/blob/master/LICENSE" }
    ],
    "screenshots": [],
    "version": "%VERSION%",
    "updated": "%TODAY%"
  },

  "routes": [
    {
      "path": "api",
      "headers": [
        {
          "name": "Authorization",
          "content": "Bearer {{ .SecureJsonData.apiToken }}"
        }
      ]
    }
  ],

  "dependencies": {
    "grafanaDependency": ">=7.0.0",
    "plugins": []
  }
}
